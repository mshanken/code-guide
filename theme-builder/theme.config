@repeater-thumbnail     : 'fuelux'; // (default/fuelux)
@spinbox                : 'fuelux'; // (default/fuelux)
@scheduler              : 'fuelux'; // (default/fuelux)
@search                 : 'fuelux'; // (default/fuelux)
@selectlist             : 'fuelux'; // (default/fuelux)
@picker                 : 'fuelux'; // (default/fuelux)
@tree                   : 'fuelux'; // (default/fuelux)
