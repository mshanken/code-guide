
/*******************************
        Theme Selection
*******************************/

/* To override a theme for an individual element
   specify theme name below
*/

/* Default Theme */
// Reset and dependencies and core default theme
// Globals
@normalize              : 'default';
@print                  : 'default';
@glyphicons             : 'default';
@colors                 : 'default';
@scaffolding            : 'default';
@type                   : 'default';
@utilities              : 'bootstrap'; // (default/bootstrap)
@responsive-utilities   : 'bootstrap'; // (default/bootstrap)
// Basics
@grid                   : 'default';
@code                   : 'default';
@tables                 : 'default';
@forms                  : 'default';
@buttons                : 'default';


/* Bootstrap Theme */
// Components
@component-animations   : 'bootstrap'; // (default/bootstrap)
@dropdowns              : 'bootstrap'; // (default/bootstrap)
@button-groups          : 'bootstrap'; // (default/bootstrap)
@input-groups           : 'bootstrap'; // (default/bootstrap)
@navs                   : 'bootstrap'; // (default/bootstrap)
@navbar                 : 'bootstrap'; // (default/bootstrap)
@breadcrumbs            : 'bootstrap'; // (default/bootstrap)
@pagination             : 'bootstrap'; // (default/bootstrap)
@pager                  : 'bootstrap'; // (default/bootstrap)
@labels                 : 'bootstrap'; // (default/bootstrap)
@badges                 : 'bootstrap'; // (default/bootstrap)
@thumbnails             : 'bootstrap'; // (default/bootstrap)
@alerts                 : 'bootstrap'; // (default/bootstrap)
@progress-bars          : 'bootstrap'; // (default/bootstrap)
@media                  : 'bootstrap'; // (default/bootstrap)
@list-group             : 'bootstrap'; // (default/bootstrap)
@panels                 : 'bootstrap'; // (default/bootstrap)
@responsive-embed       : 'bootstrap'; // (default/bootstrap)
@wells                  : 'bootstrap'; // (default/bootstrap)
@close                  : 'bootstrap'; // (default/bootstrap)
// Modules
@modals                 : 'bootstrap'; // (default/bootstrap)
@tooltip                : 'bootstrap'; // (default/bootstrap)
@popovers               : 'bootstrap'; // (default/bootstrap)
@carousel               : 'bootstrap'; // (default/bootstrap)
// Views
@jumbotron              : 'bootstrap'; // (default/bootstrap)


/* Font Awesome */
// Globals
@path                   : 'fontawesome'; // (default/fontawesome)
@core                   : 'fontawesome'; // (default/fontawesome)
@icons                  : 'fontawesome'; // (default/fontawesome)
@bordered-pulled        : 'fontawesome'; // (default/fontawesome)
@larger                 : 'fontawesome'; // (default/fontawesome)
@fixed-width            : 'fontawesome'; // (default/fontawesome)
@animated               : 'fontawesome'; // (default/fontawesome)
@rotated-flipped        : 'fontawesome'; // (default/fontawesome)
@stacked                : 'fontawesome'; // (default/fontawesome)
// Basics
@list                   : 'fontawesome'; // (default/fontawesome)


/* Jasny */
// Basics
@grid-container-smooth  : 'jasny'; // (default/jasny)
// Components
@alerts-fixed           : 'jasny'; // (default/jasny)
@button-labels          : 'jasny'; // (default/jasny)
@nav-tab-alignment      : 'jasny'; // (default/jasny)
@navmenu                : 'jasny'; // (default/jasny)
// Modules
@fileinput              : 'jasny'; // (default/jasny)
@inputmask              : 'jasny'; // (default/jasny)
@offcanvas              : 'jasny'; // (default/jasny)
@rowlink                : 'jasny'; // (default/jasny)
@transition             : 'jasny'; // (default/jasny)
@wizard                 : 'jasny'; // (default/jasny)


