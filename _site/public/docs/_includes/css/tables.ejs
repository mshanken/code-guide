        &lt;&#x2F;tr&gt;
      &lt;&#x2F;thead&gt;
      &lt;tbody&gt;
        &lt;tr&gt;
          &lt;th scope=&quot;row&quot;&gt;1&lt;&#x2F;th&gt;
          &lt;td&gt;Mark&lt;&#x2F;td&gt;
          &lt;td&gt;Otto&lt;&#x2F;td&gt;
          &lt;td&gt;@mdo&lt;&#x2F;td&gt;
        &lt;&#x2F;tr&gt;
        &lt;tr&gt;
          &lt;th scope=&quot;row&quot;&gt;2&lt;&#x2F;th&gt;
